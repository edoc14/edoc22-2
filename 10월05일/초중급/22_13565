#pragma warning (disable:4996)
#include <stdio.h>
#include <utility>
#include <queue>
using namespace std;

int map[1009][1009];
int dx[4] = { 1, 0, -1, 0 };
int dy[4] = { 0, 1, 0, -1 };
queue<pair<int, int>> Q;

int main() {

	int n, m;
	int flag = 0;
	scanf("%d %d", &n, &m);

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			scanf("%1d", &map[i][j]);
		}
	}


	for (int i = 0; i < m; i++) {
		if (map[0][i] == 0) {
			Q.push({ 0, i });
		}
		while (!Q.empty()) {
			int x = Q.front().first;
			int y = Q.front().second;
			//printf("\n%d %d\n", x, y);
			Q.pop();
			map[x][y] = 2;
			if (x == n - 1) {
				flag = 1;
				break;
			}
			for (int k = 0; k < 4; k++) {
				int X = x + dx[k];
				int Y = y + dy[k];
				if (X >= 0 && Y >= 0 && X < n && Y < m && map[X][Y] == 0) {
					Q.push({ X, Y });
				}
			}
		}
		if (flag == 1) {
			printf("YES");
			break;
		}
	}
	if (flag == 0) {
		printf("NO");
	}

	return 0;
}
